			"name": "Scenario3-language API",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Check if South African Sign Language (SASL) is included in official languages\", function () {\r",
							"    const response = pm.response.json();\r",
							"\r",
							"    // Assuming only one country (South Africa) is returned\r",
							"    const languages = response[0]?.languages;\r",
							"\r",
							"    // Convert the values of the languages object to an array\r",
							"    const languageList = Object.values(languages);\r",
							"\r",
							"    // Check if \"South African Sign Language\" is in the list\r",
							"    pm.expect(languageList).to.include(\"South African Sign Language\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
